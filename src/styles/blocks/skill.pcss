.skill {
    position: relative;
    width: 143px;
    height: 143px;

    .skill__title {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
    }
}

.skill__circle--above {
    stroke-dasharray: 300;
    stroke-width: 20px;
    fill: transparent;
}

.skill__circle--under {
    stroke: #e9e9e9;
    stroke-width: 10px;
    fill: transparent;
}

.skills__content {
    display: flex;
    padding-top: 270px;
}