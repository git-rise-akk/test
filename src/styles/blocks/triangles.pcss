.triangles {
    display: flex;
    color: #fff;
}

.triangle {
    fill: currentColor;
}

.triangles__item {
    width: 50%;
    height: 125px;
}

.reviews__triangles {
    position: absolute;
    left: 0;
    right: 0;
    background: #fff;

    .triangle__reviews {
        fill: #ebf7f6;
    }
}

